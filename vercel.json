{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/api/chat$",
      "dest": "/api/chat.js"
    },
    {
      "src": "^/api/health$",
      "dest": "/api/health.js"
    },
    {
      "src": "^/$",
      "dest": "/index.html"
    }
  ]
}
