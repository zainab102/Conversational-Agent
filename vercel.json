{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/api/chat$",
      "dest": "/api/chat.js"
    },
    {
      "src": "^/api/health$",
      "dest": "/api/health.js"
    },
    {
      "src": "^/$",
      "dest": "/index.html"
    }
  ]
}
